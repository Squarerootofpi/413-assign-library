@{
}

@model Book
<tr>
    <td class="card-title">@Model.Title</td>
    <td>@Model.AuthorLastName</td>
    <td>@Model.AuthorFirstNames</td>
    <td>@Model.Publisher</td>
    <td>$@Model.Price</td>
    <td>@Model.TotalPages</td>
    <td>@Model.Classification</td>
    <td>@Model.Category</td>
    <td class="text-muted">@Model.ISBN</td>
</tr>